<view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" scroll-x><block a:for="{{tabBars}}" a:for-item="tab" a:for-index="index" a:key="id"><block><view data-event-opts="{{[['tap',[['tabtap',[index]]]]]}}" class="{{(('swiper-tab-list')+' '+((tabIndex==index)?'active':''))}}" onTap="__e">{{''+tab.name+''}}<view class="swiper-tab-line"></view></view></block></block></scroll-view></view>