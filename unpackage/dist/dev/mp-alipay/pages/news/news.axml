<view><news-nav-bar vue-id="1" tabBars="{{tabBars}}" tabIndex="{{tabIndex}}" data-event-opts="{{[['^changTab',[['changTab']]]]}}" onChangTab="__e" onVueInit="__l"></news-nav-bar><view class="uni-tab-bar"><swiper class="swiper-box" style="{{'height:'+(swiperheight+'px')+';'}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['tabChange',['$event']]]]]}}" onChange="__e"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrollToLower',[['loadmore',['$event']]]]]}}" onScrollToLower="__e"><block a:for="{{guanzhu.list}}" a:for-item="item" a:for-index="index" a:key="index"><block><common-list vue-id="{{'2-'+index}}" item="{{item}}" index="{{index}}" onVueInit="__l"></common-list></block></block><load-more vue-id="3" loadtext="{{guanzhu.loadtext}}" onVueInit="__l"></load-more></scroll-view></swiper-item><swiper-item><scroll-view class="list" scroll-y>话题</scroll-view></swiper-item></swiper></view></view>